// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model customer{
  CustomerID String @id
  FirstName String
  LastName String 
  Email String
  PhoneNumber String
  OrderCategory String
  Description String
}
